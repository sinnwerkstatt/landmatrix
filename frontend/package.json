  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2017": true
    },
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {}
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended",
      "@vue/typescript/recommended",
      "prettier"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/no-unused-vars": "error",
      "vue/no-unused-vars": "error",
      "vue/no-v-html": "warn",
      "vue/max-attributes-per-line": [
        "warn",
        {
          "singleline": 4,
          "multiline": 1
        }
      ]
    }
  },
  "browserslist": [
    "since 2017-06"
  ]
